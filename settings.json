{
  "typescript.preferences.importModuleSpecifier": "relative",
  "javascript.preferences.importModuleSpecifier": "relative",
  
  "workbench.colorCustomizations": {
    "minimap.findMatchHighlight": "#65e1f4e1",
    "editorSuggestWidget.selectedBackground": "#20749b",
    "editorSuggestWidget.background": "#3b3b3b",
    "editorLineNumber.foreground": "#707070",
    "editorLineNumber.activeForeground": "#eaeaea",
    "editor.foldBackground": "#0a3882f1",
    "editorHoverWidget.background": "#393939",
    "editor.findMatchBorder": "#65e1f4",
    "editor.findMatchHighlightBorder": "#65e1f4",

    // "editor.findMatchForeground": "#c41dd4",
    // "editor.findMatchHighlightBackground": "#65e1f493",
    "editor.findMatchBackground": "#ffffff1e", // 현재
    // "editor.findMatchHighlightBackground": "#ffffff0f", // 전체

    // "editor.findMatchHighlightForeground": "#e53e3e",
    "editor.lineHighlightBackground": "#46464648",
    "quickInput.background": "#000000",
    "quickInput.foreground": "#ffffff",
    "quickInputList.focusForeground": "#ffffff",
    "quickInputList.focusBackground": "#424242",
    "statusBar.foreground": "#afafaf",
    "terminal.findMatchBackground": "#65e1f4e1",
    "terminal.selectionBackground": "#73737373",
    "selection.background": "#79c5c58e",
    "editorWidget.background": "#454545",
    "settings.textInputBackground": "#323232",
    "settings.checkboxBackground": "#323232",
    "settings.dropdownBackground": "#323232",
    "settings.numberInputBackground": "#323232",
    "panel.background": "#2b2b2b",
    "panelTitle.inactiveForeground": "#b5b5b5",
    "terminal.background": "#000000",
    "gitDecoration.modifiedResourceForeground": "#e9e9e9",
    "sideBar.foreground": "#b4b4b4",
    "sideBar.background": "#2a2a2a",
    "gitDecoration.ignoredResourceForeground": "#808080",
    "tab.border": "#7c7c84c1",
    "tab.inactiveForeground": "#b4b4b4",
    "tab.activeBackground": "#85858539",
    "tab.activeBorder": "#7c7c84c1",
    "tab.activeBorderTop": "#7c7c84c1",
    "[Material Theme Darker High Contrast]": {}
  },
  "editor.wordWrap": "on",
  "editor.tabSize": 2,
  "workbench.iconTheme": "material-icon-theme",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "workbench.editor.enablePreview": false,

  // "editor.codeActionsOnSave": {
  //   "source.fixAll": true
  // },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.eol": "\n",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  // "prettier.semi": false,
  // "prettier.arrowParens": "always",
  // "prettier.arrowParens": "avoid",
  // "prettier.printWidth": 100,
  // "prettier.tabWidth": 4,

  "indentRainbow.tabmixColor": "rgba(128,32,96,0.6)",
  "indentRainbow.colors": [
    "rgba(255,255,0,0.2)",
    "rgba(255,127,255,0.2)",
    "rgba(80,255,80,0.2)",
    "rgba(255,200,255,0.2)"
  ],
  "launch": {
    "configurations": [],
    "compounds": []
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "editor.tokenColorCustomizations": {
    "comments": "#00ff2f"
    "textMateRules": [
      {
        "scope": "comment, punctuation.definition.comment.js, punctuation.definition.comment.ts, punctuation.definition.comment.tsx, punctuation.definition.comment.xml, punctuation.definition.comment.json",
        "settings": {
          "foreground": "#267332"
        }
      }
    ]
  },
  "liveSassCompile.settings.generateMap": false,

  "liveSassCompile.settings.formats": [
    {
      "format": "expanded", //compressed
      "extensionName": ".css",
      "savePath": "./css"
    }
  ],

  "liveSassCompile.settings.autoprefix": ["> 1%", "last 2 versions"],
  "liveSassCompile.settings.includeItems": [],
  "liveSassCompile.settings.excludeList": ["**/node_modules/**", ".vscode/**"],
  "liveServer.settings.donotShowInfoMsg": true,
  "vscode-color-picker.languages": ["javascriptreact", "python", "javascript", "typescript"],

  "errorLens.statusBarIconsEnabled": true,
  "errorLens.gutterIconsEnabled": true,
  "errorLens.gutterIconSet": "circle",
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "prettier.printWidth": 100,
  "remote.SSH.remotePlatform": {
    "csh111.mini_shopping_game.draft---festive-sutherland.csb": "linux"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "update.mode": "manual",
  "workbench.colorTheme": "Community Material Theme"
  // "json.schemas": [{ "exclude": ["node_modules"] }]
}
